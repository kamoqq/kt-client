{
  "name": "kt-client",
  "version": "0.8.0",
  "description": "KyotoTycoon client for Node.js",
  "main": "index.js",
  "scripts": {
    "pretest": "eslint index.js ./lib ./test",
    "test": "NODE_ENV=test YOURPACKAGE_COVERAGE=1 mocha --compilers js:espower-babel/guess --require blanket --reporter mocha-lcov-reporter ./test | coveralls; mocha --compilers js:espower-babel/guess ./test"
  },
  "config": {
    "blanket": {
      "pattern": [
        "index.js",
        "lib"
      ],
      "data-cover-never": [
        "node_modules",
        "test"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kamoqq/node-kt-client.git"
  },
  "keywords": [
    "KyotoTycoon"
  ],
  "author": "kamoqq",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kamoqq/node-kt-client/issues"
  },
  "homepage": "https://github.com/kamoqq/node-kt-client",
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-eslint": "^5.0.0",
    "babel-plugin-syntax-async-functions": "^6.5.0",
    "babel-plugin-transform-regenerator": "^6.6.5",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "^6.6.0",
    "blanket": "1.1.9",
    "coveralls": "^2.11.8",
    "eslint": "~2.2.0",
    "eslint-config-airbnb": "^6.1.0",
    "espower-babel": "^4.0.1",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.2.0",
    "power-assert": "^1.3.1"
  },
  "dependencies": {
    "bl": "^1.0.0"
  }
}
