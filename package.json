{
  "name": "kt-client",
  "version": "0.8.0",
  "description": "KyotoTycoon client for Node.js",
  "main": "index.js",
  "scripts": {
    "pretest": "eslint index.js ./lib ./test",
    "test": "NODE_ENV=test YOURPACKAGE_COVERAGE=1 mocha --require blanket --reporter mocha-lcov-reporter ./test | coveralls; mocha ./test"
  },
  "config": {
    "blanket": {
      "pattern": [
        "index.js",
        "lib"
      ],
      "data-cover-never": [
        "node_modules",
        "test"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kamoqq/node-kt-client.git"
  },
  "keywords": [
    "KyotoTycoon"
  ],
  "author": "kamoqq",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kamoqq/node-kt-client/issues"
  },
  "homepage": "https://github.com/kamoqq/node-kt-client",
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "babel-plugin-transform-regenerator": "^6.9.0",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-register": "^6.9.0",
    "blanket": "1.1.9",
    "coveralls": "^2.11.8",
    "eslint": "^2.12.0",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.8.1",
    "mocha": "^2.5.3",
    "mocha-lcov-reporter": "^1.2.0",
    "power-assert": "^1.4.1"
  },
  "dependencies": {
    "bl": "^1.0.0"
  }
}
