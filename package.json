{
  "name": "kt-client",
  "version": "0.2.0",
  "description": "KyotoTycoon client for Node.js",
  "main": "index.js",
  "scripts": {
    "pretest": "eslint index.js ./lib",
    "test": "NODE_ENV=test YOURPACKAGE_COVERAGE=1 mocha --require blanket --reporter mocha-lcov-reporter ./test | coveralls; mocha ./test"
  },
  "config": {
    "blanket": {
      "pattern": [
        "index.js",
        "lib"
      ],
      "data-cover-never": [
        "node_modules",
        "test"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kamoqq/kt-client.git"
  },
  "keywords": [
    "KyotoTycoon"
  ],
  "author": "kamoqq",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kamoqq/kt-client/issues"
  },
  "homepage": "https://github.com/kamoqq/kt-client",
  "devDependencies": {
    "blanket": "^1.1.7",
    "chai": "^2.3.0",
    "coveralls": "^2.11.2",
    "eslint": "^1.8.0",
    "eslint-config-airbnb": "^0.1.0",
    "mocha": "^2.2.5",
    "mocha-lcov-reporter": "0.0.2"
  },
  "dependencies": {
    "bl": "^0.9.4"
  }
}
